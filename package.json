{
  "name": "f-utility",
  "version": "1.0.1",
  "description": "utility / futility / useful? / pointfree?",
  "main": "index.js",
  "nyc": {
    "exclude": [
      "node_modules"
    ]
  },
  "scripts": {
    "test": "npm run lint && npm run build",
    "test-es6": "ava --esnext ./tests/**/spec-*.js",
    "test-core": "npm run transpile && npm run test-es6",
    "test-log": "npm run test-core > test-output.log",
    "cover": "nyc --exclude=node_modules --exclude=tests --r=lcov --r=json --r=html --r=text --r=text-summary npm run test-core",
    "report": "istanbul report",
    "transpile": "babel src -d .",
    "build": "npm run cover",
    "lint": "./node_modules/.bin/eslint -c ./.eslintrc src/** --rule 'no-console: 2'",
    "publish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brekk/f-utility.git"
  },
  "ava": {
    "babel": "inherit",
    "require": [
      "babel-register"
    ],
    "files": [
      "tests/*.js",
      "tests/**/*.js"
    ],
    "source": [
      "src/*.js",
      "src/**/*.js"
    ],
    "concurrency": 10,
    "failFast": true
  },
  "keywords": [],
  "author": "brekk",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/brekk/f-utility/issues"
  },
  "homepage": "https://github.com/brekk/f-utility#readme",
  "dependencies": {
    "folktale": "^1.0.0-SNAPSHOT",
    "lodash": "^4.15.0"
  },
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-async-to-generator": "^6.8.0",
    "babel-plugin-transform-es2015-destructuring": "^6.9.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-2": "^6.13.0",
    "babel-register": "^6.14.0",
    "chalk": "^1.1.3",
    "eslint": "^3.5.0",
    "eslint-config-standard": "^6.0.0",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-lodash-fp": "^2.0.1",
    "eslint-plugin-promise": "^2.0.1",
    "eslint-plugin-react": "^6.2.0",
    "eslint-plugin-standard": "^2.0.0",
    "faucet": "0.0.1",
    "istanbul": "^0.4.5",
    "must": "^0.13.2",
    "nyc": "^8.1.0"
  }
}
